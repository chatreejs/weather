{
  "name": "weather",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "start:develop": "env-cmd -f .env.develop vite",
    "build": "tsc && vite build",
    "build:develop": "tsc && env-cmd -f .env.develop vite build",
    "build:prod": "tsc && env-cmd -f .env.prod vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage .",
    "test:debug": "jest --inspect-brk --runInBand --no-cache",
    "lint": "eslint src",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "antd": "5.14.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "rxjs": "^7.8.1",
    "styled-components": "^5.3.9",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^4.1.0",
    "env-cmd": "^10.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "vite": "^4.5.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.17.4",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
